<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Alastair Wyse</Authors>
    <Company />
    <Copyright>Copyright Â© Alastair Wyse 2023</Copyright>
    <PackageProjectUrl>https://github.com/alastairwyse/ApplicationAccess/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/alastairwyse/ApplicationAccess/</RepositoryUrl>
    <Description>Classes which distribute the operation and workload of an AccessManager implementation between multiple computers.</Description>
