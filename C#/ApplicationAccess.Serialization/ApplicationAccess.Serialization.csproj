<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <NoDefaultLaunchSettingsFile>true</NoDefaultLaunchSettingsFile>
    <Authors>Alastair Wyse</Authors>
    <Company>Alastair Wyse</Company>
    <Copyright>Copyright Â© Alastair Wyse 2021</Copyright>
    <Description>Classes which serialize and deserialize data associated with AccessManager classes to and from formats suitable for sending remotely.</Description>
    <PackageProjectUrl>https://github.com/alastairwyse/ApplicationAccess/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/alastairwyse/ApplicationAccess/</RepositoryUrl>
    <PackageReleaseNotes>First release.</PackageReleaseNotes>
    <PackageTags>application access authorization authorisation permission permissions user group users groups serialize deserialize serialization deserialization json</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>ApplicationAccess.Serialization.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ApplicationAccess\ApplicationAccess.csproj" />
  </ItemGroup>

</Project>
