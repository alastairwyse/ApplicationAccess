{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AccessManager": {
    "StoreBidirectionalMappings": true
  },

  "ErrorHandling": {
    "IncludeInnerExceptions": false,
    "OverrideInternalServerErrors": true,
    "InternalServerErrorMessageOverride": "An internal server error occurred"
  }
}
