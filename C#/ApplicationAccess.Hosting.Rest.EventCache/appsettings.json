{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",

  "EventCaching": {
    "CachedEventCount":  100
  },

  "ErrorHandling": {
    "IncludeInnerExceptions": false,
    "OverrideInternalServerErrors": true,
    "InternalServerErrorMessageOverride": "An internal server error occurred"
  }
}
